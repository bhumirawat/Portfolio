{
  "version": 2,
  "name": "portfolio-client",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
  { "src": "/(.*\\.(js|mjs|css|png|jpg|jpeg|svg|ico|webp|json|map|pdf))", "dest": "/$1" },
  { "src": "/.*", "dest": "/index.html" }
],

  "env": {
    "NODE_ENV": "production"
  }
}